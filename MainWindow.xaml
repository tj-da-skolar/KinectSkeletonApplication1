<Window x:Class="KinectSkeletonApplication1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="600" Width="800">
    <Grid>
        <Image Name="videoImage"></Image>
        <Canvas Background="Transparent">
            <Ellipse Fill="Green" Height="20" Width="20" Name="leftHand" Stroke="White" />
            <Ellipse Fill="Green" Height="20" Width="20" Name="rightHand" Stroke="White" />
            <Ellipse Fill="Yellow" Height="20" Width="20" Name="head" Stroke="White" />
            <Ellipse Fill="Red" Height="20" Width="20" Name="rightShoulder" Stroke="White" />
            <Ellipse Fill="Red" Height="20" Width="20" Name="leftShoulder" Stroke="White" />
            <Ellipse Fill="White" Height="20" Width="20" Name="rightKnee" Stroke="Black" />
            <Ellipse Fill="White" Height="20" Width="20" Name="leftKnee" Stroke="Black" />
            <Ellipse Fill="Violet" Height="20" Width="20" Name="rightFoot" Stroke="White" />
            <Ellipse Fill="Violet" Height="20" Width="20" Name="leftFoot" Stroke="White" />
            <Ellipse Fill="Blue" Height="20" Width="20" Name="rightElbow" Stroke="White" />
            <Ellipse Fill="Blue" Height="20" Width="20" Name="leftElbow" Stroke="White" />
            <Ellipse Fill="Gold" Height="20" Width="20" Name="spine" Stroke="White" />
            

        </Canvas>
    </Grid>
</Window>
